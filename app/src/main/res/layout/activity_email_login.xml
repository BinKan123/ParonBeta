<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/slide_bg"
    tools:context="com.peppypals.paronbeta.EmailLoginActivity">


    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="329dp"
        android:layout_height="275dp"
        app:srcCompat="@drawable/rectangle_2"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="-24dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintVertical_bias="0.11"
        tools:layout_editor_absoluteY="50dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="116dp"
        android:layout_height="103dp"
        app:srcCompat="@drawable/logo"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView5"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        android:layout_marginBottom="191dp" />

    <EditText
        android:id="@+id/emailText"
        android:layout_width="293dp"
        android:layout_height="47dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="93dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Email"
        app:layout_constraintLeft_toLeftOf="@+id/imageView5"
        app:layout_constraintRight_toRightOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/imageView5" />

    <EditText
        android:id="@+id/passwordText"
        android:layout_width="293dp"
        android:layout_height="47dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="27dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="LÃ–SENORD"
        app:layout_constraintBottom_toTopOf="@+id/forgotPw"
        app:layout_constraintLeft_toLeftOf="@+id/emailText"
        app:layout_constraintRight_toRightOf="@+id/emailText"
        app:layout_constraintTop_toBottomOf="@+id/emailText"
        app:layout_constraintVertical_bias="0.096" />

    <TextView
        android:id="@+id/forgotPw"
        android:layout_width="130dp"
        android:layout_height="23dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="TextView"
        android:textAlignment="center"
        app:layout_constraintLeft_toLeftOf="@+id/imageView5"
        app:layout_constraintRight_toRightOf="@+id/imageView5"
        tools:layout_editor_absoluteY="302dp" />

    <Button
        android:id="@+id/emailLogin"
        android:layout_width="329dp"
        android:layout_height="45dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/roundedbtn"
        android:backgroundTint="@color/login_bg_btn"
        android:text="Log in"
        app:layout_constraintLeft_toLeftOf="@+id/imageView5"
        app:layout_constraintRight_toRightOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/forgotPw" />

    <TextView
        android:id="@+id/eller"
        android:layout_width="122dp"
        android:layout_height="20dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="0dp"
        android:text="TextView"
        android:textAlignment="center"
        app:layout_constraintLeft_toLeftOf="@+id/forgotPw"
        app:layout_constraintRight_toRightOf="@+id/forgotPw"
        tools:layout_editor_absoluteY="398dp"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/loginWithFB"
        android:layout_width="329dp"
        android:layout_height="43dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/facebook"
        app:layout_constraintHorizontal_bias="0.454"
        app:layout_constraintLeft_toLeftOf="@+id/emailLogin"
        app:layout_constraintRight_toRightOf="@+id/emailLogin"
        app:layout_constraintTop_toBottomOf="@+id/eller" />

    <Button
        android:id="@+id/loginWithGmail"
        android:layout_width="329dp"
        android:layout_height="41dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/gmaillog"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintLeft_toLeftOf="@+id/loginWithFB"
        app:layout_constraintRight_toRightOf="@+id/loginWithFB"
        app:layout_constraintTop_toBottomOf="@+id/loginWithFB"
        tools:layout_editor_absoluteX="48dp" />

    <TextView
        android:id="@+id/registerHere"
        android:layout_width="187dp"
        android:layout_height="20dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textAlignment="center"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loginWithGmail"
        app:layout_constraintHorizontal_bias="0.502" />
</android.support.constraint.ConstraintLayout>
